{
  "name": "greeting-service",
  "version": "1.0.0",
  "description": "A greeting service with Express ",
  "main": "lib/index.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-2\"",
    "prestart": "npm run build",
    "start": "nodemon lib/index.js",
    "prepublish": "babel src -s -D -d lib --presets es2015,stage-2",
    "build": "babel src -s -D -d lib --presets es2015,stage-2",
    "test": "mocha --require babel-core/register  --recursive",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive --compilers js:babel-core/register --colors --reporter dot test/"
  },
  "keywords": [
    "Express",
    "REST",
    "NodeJS"
  ],
  "author": "Marcelo",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0", 
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0", 
    "nodemon": "^1.11.0",
    "supertest": "^2.0.1"
  }
}
